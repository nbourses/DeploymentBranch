<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.nbourses.oyeok.fragments.AddListingFinalCard">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/white"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:orientation="horizontal"
            android:background="@color/greenish_blue">
     <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Listing Details"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginLeft="100dp"
        android:layout_gravity="center"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cancel"
                android:textColor="@color/black"
                android:textStyle="bold|italic"
                android:textSize="14dp"
                android:layout_marginLeft="60dp"
                android:layout_gravity="center_vertical|right"/>
        </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dip"
                android:layout_margin="5dip"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="2.5 BHK"
                    android:textColor="@color/greenish_blue"
                    android:layout_marginLeft="20dip"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:id="@+id/config"
                    android:textSize="22dip"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dip"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:text="(approx area : "/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/approx_area"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:text="1400"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:layout_marginLeft="5dip"
                    android:text="sqft)"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="67dp"
                    android:layout_marginLeft="4dp"
                    android:background="@drawable/buildingiconbeforeclick"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:gravity="left">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Himalayan Heights CHS"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:textSize="18dip"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="50dip"
                        android:layout_marginTop="2dip"
                        android:textColor="@color/gray"
                        android:text="My Address"/>
                </LinearLayout>

            </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dip"
            android:background="@color/greenish_blue"
            android:layout_marginTop="4dip"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:gravity="center"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.kyleduo.switchbutton.SwitchButton
                    android:id="@+id/toggleBtn"
                    android:layout_width="54dp"
                    android:layout_height="27dp"
                    android:checked="true"
                    app:kswThumbColor="@color/greenish_blue"
                    android:layout_gravity="center"/>
            <!--<Switch
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColorLink="@color/greenish_blue"
                android:gravity="center"
                android:checked="true"
                />-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:id="@+id/transaction_type"
                android:text="RENT"
                android:textColor="@color/greenish_blue"
                android:textSize="18dip"
                android:layout_marginTop="5dp"
                android:textStyle="bold|italic"/>
                </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="30dip">
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Requirement"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold|italic"
                    android:id="@+id/req"
                    />
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Availability"
                    android:textSize="18dp"
                    android:textColor="@color/black"
                    android:layout_marginTop="5dp"
                   android:id="@+id/availability"
                    android:textStyle="bold|italic"
                    />
            </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="2dip"
            android:background="@color/greenish_blue"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="5dp">

            <TextView
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="Furnishing status"
                android:gravity="center_horizontal|center_vertical"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp"
                android:textSize="14sp"
                android:textColor="@color/gray"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="Possession Date"
                android:layout_marginTop="4dp"
                android:textSize="14sp"
                android:textColor="@color/gray"
                android:gravity="center_horizontal|center_vertical"
                android:textStyle="bold"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1dp"
            android:orientation="horizontal">

            <Spinner
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="-5dp"
                android:id="@+id/spinner"
                style="@style/spinner_style1"
                />

            <TextView
                android:layout_width="140dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="5dp"
                android:id="@+id/txtcalendar1"
                android:paddingRight="10dp"
                android:textSize="16dp"
                android:gravity="center_horizontal|center_vertical"
                android:background="@drawable/calendar_bg"
                android:text="28-7-2016"
                android:textColor="@color/Black"
                android:textStyle="bold"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dip"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip">

                <TextView
                    android:layout_width="100dip"
                    android:layout_height="match_parent"
                    android:text="20,000"
                    android:gravity="left"
                    android:layout_marginLeft="1dp"
                    android:id="@+id/min"
                    android:textColor="@color/black"
                    android:textStyle="bold"/>
                <TextView
                    android:layout_width="100dip"
                    android:layout_height="match_parent"
                    android:textColor="@color/black"
                    android:layout_marginLeft="50dip"
                    android:id="@+id/max"
                    android:gravity="right"
                    android:textStyle="bold"
                    android:text="20,000"/>
            </LinearLayout>
        <SeekBar
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dip"
            android:id="@+id/seekBar" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="min"
                    android:layout_marginLeft="2dp"
                    android:textColor="@color/black"
                    android:layout_marginTop="-5dip"
                    android:textStyle="bold"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_marginLeft="200dip"
                    android:layout_marginTop="-5dip"
                    android:textStyle="bold"
                    android:text="max"/>
            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="10dp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="28dip"
                android:text="58,000"
                android:id="@+id/selected_rate"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="14dip"
                android:text="/month"
                android:id="@+id/textView16"
                android:layout_alignBottom="@+id/selected_rate"
                android:layout_toRightOf="@+id/selected_rate"
                android:layout_toEndOf="@+id/selected_rate" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18dip"
                android:text="@string/Rs"
                android:id="@+id/textView14"
                android:layout_marginRight="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="4dp"
                android:layout_alignTop="@+id/selected_rate"
                android:layout_toLeftOf="@+id/selected_rate"
                android:layout_toStartOf="@+id/selected_rate" />

        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:orientation="horizontal"
            android:layout_gravity="bottom"
            android:gravity="center"
            android:layout_margin="5dp"
            android:background="@color/greenish_blue">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dip"
                android:text="post"
                android:gravity="bottom"
                android:textColor="@color/black"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="4dip"
                android:text="Requirement"
                android:gravity="center"
                android:textColor="@color/black"
                android:textSize="20dip"
                android:textStyle="bold"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="20dip"
                android:text="to Brokers"
                android:gravity="bottom"
                android:layout_marginLeft="4dip"
                android:textColor="@color/black" />

        </LinearLayout>
    </LinearLayout>


</FrameLayout>
