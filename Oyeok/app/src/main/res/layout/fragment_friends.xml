<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">


        <LinearLayout
            android:id="@+id/recordWorkout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:layout_marginTop="@dimen/sixteen_dp"
            android:background="@drawable/rectable_border_effect"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="24dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/eight_dp"
                android:layout_marginRight="@dimen/eight_dp"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical"
                android:weightSum="1">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/eight_dp"
                    android:paddingTop="@dimen/eight_dp"
                    android:src="@drawable/ic_room_24dp" />

            </LinearLayout>

            <ImageView
                android:layout_width="1dp"
                android:layout_height="fill_parent"
                android:background="#ccc" />


            <LinearLayout
                android:id="@+id/route"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_margin="@dimen/eight_dp"
                android:layout_weight="1"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical"
                android:weightSum="1">


                <TextView
                    android:id="@+id/inputSearch"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:inputType="text"
                    android:maxLines="2"
                    android:paddingBottom="@dimen/eight_dp"
                    android:paddingTop="@dimen/eight_dp"
                    android:text="Navratna Appt plot no 51 flat no 306Nerul Navimumbai 400706 Maharashtra india "
                    android:textColor="@color/white"

                    android:textColorHint="@color/white" />


            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/textforoye"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/recordWorkout"
            android:layout_margin="@dimen/sixteen_dp"
            android:text="Oye a broker to"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="@dimen/twenty_four" />


        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/oye_select"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textforoye"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_home_oye_intent_specs"
                android:id="@+id/icon_home" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_shop_oye_intent_specs"
                android:id="@+id/icon_shop" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_industrial_oye_intent_specs"
                android:id="@+id/icon_industrial" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_office_oye_intent_specs"
                android:id="@+id/icon_office" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_others_oye_intent_specs"
                android:id="@+id/icon_others" />

            <!--<Spinner
                android:id="@+id/static_spinner"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginRight="@dimen/four_dp"
                android:layout_weight="1"
                android:entries="@array/intent"
                android:paddingLeft="0dp"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/dynamic_spinner"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/four_dp"
                android:layout_weight="1"
                android:entries="@array/Purpose"
                android:spinnerMode="dropdown" />-->

        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/border_linespinner"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/oye_select"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:orientation="horizontal">

            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginRight="@dimen/four_dp"
                android:layout_weight="1"
                android:background="@color/colorPrimary" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginLeft="@dimen/four_dp"
                android:layout_weight="1"
                android:background="@color/colorPrimary" />
        </LinearLayout>

        <TextView
            android:id="@+id/fora"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/sixteen_dp"
            android:text="Purpose"
            android:layout_below="@+id/border_linespinner"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="@dimen/twenty_four" />

        <RadioGroup
            android:id="@+id/RadioBtnGroup"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:weightSum="2"
            android:layout_below="@+id/fora"
            android:layout_marginBottom="@dimen/sixteen_dp"
            >



            <!--<RadioButton
                android:id="@+id/prepaidRadio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:checked="true"
                android:text="Rent"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/sixteen_sp" />

            <RadioButton
                android:id="@+id/postpaidRadio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Sale"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/sixteen_sp" />-->

        </RadioGroup>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/layout_container"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textforoye"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:orientation="horizontal">






        </LinearLayout>


        <com.nbourses.oyeok.RPOT.PriceDiscovery.UI.rangeSeekBarBrokerTypePanel.RangeSeekbar_roomprice
            xmlns:seekbar="http://schemas.android.com/apk/res-auto"
            android:id="@+id/seekbarprice"
            android:layout_width="fill_parent"
            android:layout_height="72dp"
            android:layout_below="@+id/RadioBtnGroup"
            android:paddingLeft="@dimen/sixteen_dp"
            android:paddingRight="@dimen/sixteen_dp"
            seekbar:leftCursorBackground="@drawable/rangeseekbar_roomprice_cursor"
            seekbar:markTextArray="@array/price"
            seekbar:rightCursorBackground="@drawable/rangeseekbar_roomprice_cursor"
            seekbar:spaceBetween="20dp"
            seekbar:textSize="@dimen/twelve_sp" />

        <!--<com.appyvet.rangebar.RangeBar-->
        <!--xmlns:custom="http://schemas.android.com/apk/res-auto"-->
        <!--android:id="@+id/rangebar"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="72dp"-->
        <!--android:layout_below="@+id/RadioBtnGroup"-->
        <!--custom:rangeBar="false"-->
        <!--android:layout_marginLeft="16dp"-->
        <!--android:layout_marginRight="16dp"/>-->

        <com.nbourses.oyeok.RPOT.PriceDiscovery.UI.rangeSeekBarBrokerTypePanel.RangeSeekbar_minmaxvalue xmlns:seekbar="http://schemas.android.com/apk/res-auto"
            android:id="@+id/seekbar"
            android:layout_width="fill_parent"
            android:layout_height="72dp"
            android:layout_below="@+id/seekbarprice"
            android:layout_marginTop="@dimen/eight_dp"
            android:paddingLeft="@dimen/sixteen_dp"
            android:paddingRight="@dimen/sixteen_dp"
            seekbar:leftCursorBackground="@drawable/rangeseekbar_roomprice_cursor"
            seekbar:markTextArray="@array/values"
            seekbar:rightCursorBackground="@drawable/rangeseekbar_roomprice_cursor"
            seekbar:spaceBetween="20dp"
            seekbar:textSize="@dimen/twelve_sp" />


        <!--<View-->
        <!--android:id="@+id/border_line"-->
        <!--android:layout_width="fill_parent"-->
        <!--android:layout_height="1dp"-->
        <!--android:background="@color/grey"-->
        <!--android:layout_above="@+id/oye"-->
        <!--/>-->

        <Button
            android:id="@+id/bt_oye"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/thirtysix_dp"
            android:layout_alignParentBottom="true"
            android:layout_margin="@dimen/sixteen_dp"
            android:layout_weight="1"
            android:background="@drawable/button_greenishblue"
            android:gravity="center"
            android:text="Oye"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="@dimen/sixteen_sp" />


    </RelativeLayout>


</RelativeLayout>
