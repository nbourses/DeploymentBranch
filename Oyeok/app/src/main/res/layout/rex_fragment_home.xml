                            android:background="@drawable/deal_circle"
                            android:textColor="@color/white"
                            android:textSize="@dimen/sixteen_sp"
                            android:gravity="center_vertical"
                            android:layout_alignParentLeft="true"/>-->

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/average_rate_this_locality"
                            android:id="@+id/tv_building"
                            android:textColor="@color/White"
                            android:singleLine="true"
                            android:layout_marginRight="66dp"
                            android:textSize="12sp"
                            android:visibility="visible"
                            android:gravity="top"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="30dp"
                            android:textStyle="bold|italic" />

                        <com.nbourses.oyeok.widgets.HorizontalPicker.HorizontalPicker
                            android:id="@+id/picker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="50dp"
                            android:layout_marginTop="10dp"
                            android:textColor="@android:color/white"
                            android:background="@android:color/black"
                            android:textSize="15sp"
                            android:ellipsize="marquee"
                            android:textStyle="bold"
                            android:marqueeRepeatLimit="1"
                            android:focusable="true"

                            app:values="@array/hpicker"
                            app:dividerSize="10dp"
                            app:sideItems="@integer/sideItems"
                            android:layout_above="@+id/tvRate" />
                        <!--android:focusableInTouchMode="true"-->
                        <TextView
                            android:id="@+id/rupeesymbol"
                            android:layout_width="wrap_content"
                            android:text="@string/Rs"
                            android:layout_marginLeft="100dp"
                            android:layout_marginBottom="4dp"
                            android:textColor="@color/greenish_blue"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tvRate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="psf"
                            android:textColor="@color/greenish_blue"
                            android:layout_alignParentBottom="true"
                            android:textSize="12sp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="120dp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/tvCommingsoon"
                            android:layout_marginRight="30dp"
                            android:layout_alignBottom="@+id/picker"
                            android:layout_alignEnd="@+id/picker"
                            android:layout_alignTop="@+id/picker"
                            android:textColor="@color/White"

                            android:foregroundGravity="center_vertical"
                            android:layout_marginLeft="50dp"
                            android:layout_marginTop="15dp" />
                        <!--    android:layout_toEndOf="@+id/tvFetchingRates"
                        android:layout_toEndOf="@+id/beacons2"-->

                        <LinearLayout
                            android:layout_width="265dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_centerVertical="true"
                            >
                                                <TextView
                                                    android:layout_width="255dp"
                                                    android:layout_height="wrap_content"
                                                    android:id="@+id/tvFetchingRates"
                                                    android:textColor="@color/White"
                                                    android:visibility="visible"
                                                    android:foregroundGravity="center"
                                                    android:gravity="center"
                                                    android:textSize="12dp"
                                                    android:textStyle="bold" />
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" >

                            <TextView
                                android:id="@+id/newVisits"
                                android:layout_width="61dp"
                                android:layout_height="61dp"
                                android:backgroundTint="@color/yellow"
                                android:text="Oye"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:background="@drawable/deal_circle"
                                android:textColor="@color/white"
                                android:textSize="20sp"
                                android:layout_alignParentRight="true"/>

                            <ImageView
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:id="@+id/beacons2"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentStart="true" />

                            <ImageView
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:id="@+id/beacons3"
                                android:layout_alignParentTop="true"
                                android:layout_toEndOf="@+id/beacons2"
                                android:layout_marginStart="189dp" />

                            <TextView
                                android:id="@+id/txtFilterValue"
                                android:layout_width="61dp"
                                android:layout_height="61dp"
                                android:backgroundTint="@color/yellow"
                                android:text="Home"
                                android:textStyle="bold|italic"
                                android:gravity="center"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:background="@drawable/deal_circle"
                                android:textColor="@color/white"
                                android:textSize="14sp"
                                android:layout_alignParentRight="true" />

                        </RelativeLayout>


                    </RelativeLayout>

                </LinearLayout>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_markerpin"
                    android:layout_below="@id/ll_marker"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="-15dp"
                    android:id="@+id/Mmarker" />

            </RelativeLayout>

