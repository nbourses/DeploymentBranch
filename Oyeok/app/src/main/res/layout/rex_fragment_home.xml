            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:visibility="gone"
                android:id="@+id/alertLayout"
                android:background="#2DC4B6"
                android:layout_gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="25dp">

                <TextView
                    android:id="@+id/errorText"
                    android:text="ss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:gravity="center_horizontal"/>

            </RelativeLayout>

        </FrameLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"

            android:orientation="horizontal"
            android:id="@+id/recordWorkout"
            android:background="@android:color/transparent"
            android:layout_margin="5dp">
            <!--@color/greenish_blue-->
            <LinearLayout
                android:layout_width="24dp"
                android:layout_height="fill_parent"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical"
                android:weightSum="1"
                android:layout_marginBottom="4dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="12dp"
                android:id="@+id/lin"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/qrCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_qrcode" />

            </LinearLayout>
            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/selected_property"
                android:src="@drawable/selected_property"
                android:foregroundGravity="left"
                android:visibility="gone"
                android:background="@android:color/transparent"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp" />

            <ImageView
                android:layout_width="1dp"
                android:layout_height="fill_parent"
                android:background="@color/white"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:weightSum="1"
                android:id="@+id/route"
                android:nestedScrollingEnabled="false">

                <AutoCompleteTextView android:id="@+id/inputSearch"
                    android:inputType="textMultiLine|textPostalAddress"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:hint="Search .."
                    android:background="@android:color/transparent"
                    android:textColorHint="@color/white"
                    android:textColor="@color/white"
                    android:textCursorDrawable="@color/white"
                    android:drawableRight="@drawable/ic_search_24dp"
                    android:singleLine="false"
                    android:minLines="2"
                    android:lines="2"
                    android:scrollbars="none"
                    android:textAlignment="center"
                    android:textSize="15dp"
                    android:nestedScrollingEnabled="false"
                    android:foregroundGravity="center" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:id="@+id/beacons1"
                    android:src="@drawable/beacon"
                    android:visibility="gone" />



            </LinearLayout>

            <ImageView
                android:layout_width="1dp"
                android:layout_height="fill_parent"
                android:background="@color/white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_margin="@dimen/eight_dp"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_gps_fixed_24dp"/>

            </LinearLayout>

        </LinearLayout>
