<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <LinearLayout
            android:id="@+id/recordWorkout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:layout_marginTop="@dimen/sixteen_dp"
            android:background="@drawable/rectable_border_effect"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="24dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/eight_dp"
                android:layout_marginRight="@dimen/eight_dp"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical"
                android:weightSum="1">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/eight_dp"
                    android:paddingTop="@dimen/eight_dp"
                    android:src="@drawable/ic_room_24dp" />

            </LinearLayout>

            <ImageView
                android:layout_width="1dp"
                android:layout_height="fill_parent"
                android:background="#ccc" />


            <LinearLayout
                android:id="@+id/route"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_margin="@dimen/eight_dp"
                android:layout_weight="1"
                android:gravity="center_vertical|center_horizontal"
                android:orientation="vertical"
                android:weightSum="1">


                <TextView
                    android:id="@+id/inputSearch"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:inputType="text"
                    android:maxLines="2"
                    android:paddingBottom="@dimen/eight_dp"
                    android:paddingTop="@dimen/eight_dp"
                    android:text="Navratna Appt plot no 51 flat no 306Nerul Navimumbai 400706 Maharashtra india "
                    android:textColor="@color/white"

                    android:textColorHint="@color/white" />


            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/textforoye"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/recordWorkout"
            android:layout_margin="@dimen/sixteen_dp"
            android:text="Oye a broker to"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="@dimen/twenty_four" />

        <RadioGroup
            android:id="@+id/RadioBtnGroupSeeShow"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:weightSum="2"
            android:layout_below="@+id/textforoye"
            android:layout_marginBottom="@dimen/sixteen_dp"
            >

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="See"
                android:id="@+id/seeRadioButton"
                android:layout_below="@+id/recordWorkout"
                android:layout_alignRight="@+id/recordWorkout"
                android:layout_alignEnd="@+id/recordWorkout"
                android:checked="false" />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Show"
                android:id="@+id/showRadioButton"
                android:layout_below="@+id/seeRadioButton"
                android:layout_alignLeft="@+id/seeRadioButton"
                android:layout_alignStart="@+id/seeRadioButton"
                android:checked="false" />
        </RadioGroup>


        <TextView
            android:id="@+id/textForRentSale"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RadioBtnGroupSeeShow"
            android:layout_margin="@dimen/sixteen_dp"
            android:text=""
            android:textColor="@color/colorPrimaryDark"
            android:textSize="@dimen/twenty_four" />

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/oye_select"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textForRentSale"
            android:clickable="true"
            android:layout_marginLeft="@dimen/sixteen_dp"
            android:layout_marginRight="@dimen/sixteen_dp"
            android:orientation="horizontal">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_room_24dp" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_home_oye_intent_specs_selector"
                android:id="@+id/icon_home" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_shop_oye_intent_specs_selector"
                android:id="@+id/icon_shop" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_industrial_oye_intent_specs_selector"
                android:id="@+id/icon_industrial" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_office_oye_intent_specs_selector"
                android:id="@+id/icon_office" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="100dp"
                android:paddingBottom="@dimen/eight_dp"
                android:paddingTop="@dimen/eight_dp"
                android:src="@drawable/ic_others_oye_intent_specs_selector"
                android:id="@+id/icon_others" />


