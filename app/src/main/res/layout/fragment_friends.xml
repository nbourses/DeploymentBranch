                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginRight="@dimen/four_dp"
                android:layout_weight="1"
                android:background="@color/colorPrimary" />

            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_marginLeft="@dimen/four_dp"
